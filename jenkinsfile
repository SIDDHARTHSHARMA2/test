node{
 withCredentials([file(credentialsId: 'basic-342606', variable: 'GC_KEY')]){
 sh "cat '$GC_KEY' | docker login -u _json_key --password-stdin https://us.gcr.io"
 sh "gcloud auth activate-service-account --key-file='$GC_KEY'"
 sh "gcloud auth configure-docker"
 GLOUD_AUTH = sh (
       script: 'gcloud auth print-access-token',
       returnStdout: true
  ).trim()
 echo "Pushing image To GCR"
 sh "docker push us.gcr.io/basic-342606/busybox:1"
}
